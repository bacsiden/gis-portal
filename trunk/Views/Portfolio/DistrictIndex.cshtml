@model List<NationalIT.District>
<div class="row-fluid">
    <div class="span12">
        <h3>Quận/Huyện</h3>
        <div class="row-fluid">
            @Html.Partial("_TopButton", "District")
        </div>
        <table class="table">
            <tr>
                <th>
                    <input type="checkbox" name="" value="" class="checkAll" />
                </th>
                <th>
                    Tên quận / huyện
                </th>
                <th>
                    Tên tỉnh
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <input type="checkbox" name="checkOwner" value="@item.ID" class="checkitem"/>
                    </td>
                    <td>
                        @Html.ActionLink(item.Title, "DistrictUpdate", new { id = item.ID })
                    </td>
                    <td>
                        @(item.Province.Title)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
