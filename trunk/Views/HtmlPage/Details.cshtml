@model NationalIT.HtmlPage
@{
    Layout = "/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Details";
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-8">
                <div class="panel-details">
                    @Html.Raw(Model.Content)
                </div>
            </div>
            <div class="col-md-4">
                @Html.Action("RightPanel", "Post", new { model = Model })
            </div>
        </div>
    </div>
</div>
